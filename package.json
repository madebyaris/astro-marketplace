{
  "name": "astro-marketplace",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"astro dev\" \"node server/index.js\"",
    "dev:astro": "astro dev",
    "dev:server": "node server/index.js",
    "build": "astro build && tsc server/index.ts --outDir dist --target es2020 --module commonjs",
    "build:astro": "astro build",
    "build:server": "tsc server/index.ts --outDir dist --target es2020 --module commonjs",
    "preview": "node dist/index.js",
    "format": "prettier -w .",
    "lint": "eslint .",
    "db:migrate:local": "wrangler d1 migrations apply astro_marketplace --local",
    "db:seed:local": "wrangler d1 execute astro_marketplace --local --file migrations/0002_seed.sql && wrangler d1 execute astro_marketplace --local --file migrations/0003_seed_admin.sql",
    "db:console:local": "wrangler d1 execute astro_marketplace --local --command \"SELECT name FROM sqlite_schema WHERE type='table' ORDER BY name;\""
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@astrojs/react": "latest",
    "@astrojs/tailwind": "latest",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "astro": "latest",
    "autoprefixer": "latest",
    "bcryptjs": "^2.4.3",
    "eslint": "latest",
    "postcss": "latest",
    "prettier": "latest",
    "tailwindcss": "latest",
    "typescript": "latest",
    "wrangler": "^4.40.0"
  }
}
